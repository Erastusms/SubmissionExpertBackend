{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": "off",
    "no-underscore-dangle": "off",
    "no-trailing-spaces": "off",
    "comma-dangle": "off",
    "class-methods-use-this": "off",
    "no-unused-vars": "off",
    "object-curly-newline": "off"
  }
}
